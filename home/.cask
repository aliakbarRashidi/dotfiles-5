#!/usr/bin/env bash
# Install native apps
brew tap phinze/homebrew-cask
brew install brew-cask

function installcask() {
  brew cask install "${@}"
}

installcask dropbox
installcask google-chrome
installcask iterm2
installcask karabiner
installcask mactex
installcask pandoc
installcask seil
installcask skype
installcask spotify
installcask the-unarchiver
